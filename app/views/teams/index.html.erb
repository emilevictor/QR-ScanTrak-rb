<h1>Listing teams</h1>

<ul id="teamsMenu">
<li><%= link_to 'New Team', new_team_path %></li>
</ul>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>

    <!--<th>User</th>-->
    <!--<th>Tag</th>-->
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @teams.each do |team| %>
  <tr>
    <td><%= team.name %></td>
    <td><%= team.description %></td>
    <!--<td><%= team.user_id %></td>-->
    <!--<td><%= team.tag_id %></td>-->
    <td><%= link_to 'Show', team %></td>
    <td><%= link_to 'Edit', edit_team_path(team) %></td>
    <td><%= link_to 'Delete', team, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

